  <!-- The NameTemplateList lists a number of name templates for the current race. -->
  <RaceNameTemplateList>
    <Race Name="Human">
      <!-- 3+ letters:  V MC V [E.g., Erdi] -->
      <NameTemplate Name="V, MC, V ['Erdi']">
          <Part Type="Cap">Vowel</Part>
          <Part Type="Normal">MidConsonant</Part>
          <Part Type="Normal">Vowel</Part>
      </NameTemplate>

      <!-- 3+ letters:  V MC DoubleVowel [E.g., Erdoo] -->
      <NameTemplate Name="V, MC, VV ['Erdoo']">
          <Part Type="Cap">Vowel</Part>
          <Part Type="Normal">MidConsonant</Part>
          <Part Type="Normal">DoubleVowel</Part>
      </NameTemplate>

      <!-- 3+ letters:  Fricative DoubleVowel Nasal [E.g., Sean] -->
      <NameTemplate Name="Fricative, VV, Nasal ['Sean']">
          <Part Type="Cap">Fricative</Part>
          <Part Type="Normal">DoubleVowel</Part>
          <Part Type="Normal">Nasal</Part>
      </NameTemplate>

      <!-- 3+ letters:  Fricative DoubleVowel Consonant [E.g., Seaz] -->
      <NameTemplate Name="Fricative, VV, C ['Seaz']">
          <Part Type="Cap">Fricative</Part>
          <Part Type="Normal">DoubleVowel</Part>
          <Part Type="Normal">Consonant</Part>
      </NameTemplate>

      <!-- 5 letters:  CVCVC [E.g., Seton] -->
      <NameTemplate Name="CVCVC ['Seton']">
          <Part Type="Cap">Consonant</Part>
          <Part Type="Normal">Vowel</Part>
          <Part Type="Normal">Consonant</Part>
          <Part Type="Normal">Vowel</Part>
          <Part Type="Normal">Consonant</Part>
      </NameTemplate>

      <!-- 3 letters:  VCV [E.g., Eti] -->
      <NameTemplate Name="V, C, V ['Eti']">
          <Part Type="Cap">Vowel</Part>
          <Part Type="Normal">Consonant</Part>
          <Part Type="Normal">Vowel</Part>
      </NameTemplate>

      <!-- 3+ letters:  V DC V [E.g., Etti] -->
      <NameTemplate Name="V, CC, V ['Etti']">
          <Part Type="Cap">Vowel</Part>
          <Part Type="Normal">DoubleConsonant</Part>
          <Part Type="Normal">Vowel</Part>
      </NameTemplate>

      <!-- 5+ letters:  Nasal V Fricative V EndingGuttural [E.g., Masher] -->
      <NameTemplate Name="Nasal, V, Fricative, V, Guttural ['Masher']">
          <Part Type="Cap">Nasal</Part>
          <Part Type="Normal">Vowel</Part>
          <Part Type="Normal">Fricative</Part>
          <Part Type="Normal">Vowel</Part>
          <Part Type="Normal">EndingNasal</Part>
      </NameTemplate>

      <!-- 5+ letters:  Fricative V DoubleGlide V EndingNasal [E.g., Sawyer] -->
      <NameTemplate Name="Fricative, V, DoubleGlide, V, EndingNasal ['Sawyer']">
          <Part Type="Cap">Fricative</Part>
          <Part Type="Normal">Vowel</Part>
          <Part Type="Normal">DoubleGlide</Part>
          <Part Type="Normal">Vowel</Part>
          <Part Type="Normal">EndingNasal</Part>
      </NameTemplate>
    </Race>

    <Race Name="Orc">
      <!-- 8+ letters:  [E.g., Gruph'Norgle] -->
      <NameTemplate Name="Orcish [Gruph Norgle]">
          <Part Type="Cap">OrcBegin</Part>
          <Part Type="Normal">Vowel</Part>
          <Part Type="Normal">Fricative</Part>
          <Part Type="Normal">Separator</Part>
          <Part Type="Cap">OrcBegin</Part>
          <Part Type="Normal">Vowel</Part>
          <Part Type="Normal">OrcMiddle</Part>
          <Part Type="Normal">Vowel</Part>
      </NameTemplate>
    </Race>
